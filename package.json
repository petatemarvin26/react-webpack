{
  "name": "react-webpack",
  "version": "1.2.3",
  "description": "This boilerplate intended for react boilerplate from scratch using webpack",
  "author": "Marvin Petate",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./build",
    "start": "npm run clean; webpack serve --config ./config/webpack/webpack.config.js",
    "build": "npm run clean; webpack --config ./config/webpack/webpack.config.js",
    "build:dev": "npm run clean; webpack --env variant=dev --config ./config/webpack/webpack.config.js",
    "build:uat": "npm run clean; webpack --env variant=uat --config ./config/webpack/webpack.config.js",
    "build:prod": "npm run clean; webpack --env variant=prod --config ./config/webpack/webpack.config.js"
  },
  "keywords": [
    "react",
    "webpack"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@svgr/webpack": "^8.1.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/parser": "^6.4.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "dotenv": "^16.3.1",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  }
}
