{
  "name": "react-webpack",
  "version": "1.1.3",
  "description": "This is React Application maintain the light weight configuration using Webpack",
  "private": true,
  "scripts": {
    "start": "webpack serve --config configs/webpack/webpack.config.js",
    "eslint": "npx eslint ./src",
    "clean": "rm -rf build",
    "build": "npm run clean; webpack build --config configs/webpack/webpack.config.js",
    "build:dev": "npm run clean; webpack build --env variant=dev --config configs/webpack/webpack.config.js",
    "build:uat": "npm run clean; webpack build --env variant=uat --config configs/webpack/webpack.config.js",
    "build:prod": "npm run clean; webpack build --env variant=prod --config configs/webpack/webpack.config.js"
  },
  "keywords": [
    "scratch",
    "react"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "author": "Marvin Petate",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@svgr/webpack": "^6.5.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-webpack-plugin": "^3.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "interpolate-html-plugin": "^4.0.0",
    "mini-css-extract-plugin": "^2.7.2",
    "process": "^0.11.10",
    "react-refresh": "^0.14.0",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.1",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^5.3.6",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^4.9.4",
    "webpack": "^5.86.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-merge": "^5.9.0"
  }
}
